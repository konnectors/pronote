  "devDependencies": {
    "cozy-app-publish": "^0.34.0",
    "cozy-jobs-cli": "2.4.2",
    "cozy-konnector-build": "1.7.0",
    "husky": "^8.0.0",
    "webpack-cli": "^5.1.4"
