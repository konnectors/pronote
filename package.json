  "description": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:ecnivtwelve/cozy-konnector-pronote.git"
  },
  "keywords": [],
  "author": "Cozy Cloud",
  "license": "AGPL-3.0",
  "eslintConfig": {
    "extends": [
      "cozy-app"
    ]
  },
  "eslintIgnore": [
    "build",
    "data"
  ],
  "scripts": {
    "start": "node ./src/index.js",
    "dev": "cozy-konnector-dev",
    "standalone": "cozy-konnector-standalone",
    "pretest": "npm run clean",
    "clean": "rm -rf ./data",
